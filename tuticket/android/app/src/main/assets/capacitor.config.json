{
  "appId": "com.latikait.ltkticket",
  "appName": "tuticket",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    }
  },
  "cordova": {},
  "server": {
    "allowNavigation": [
          {
            "url": "apptkapi.latikait.com.ar",
            "injectJs": "true"
          },
          {
            "url": "10.0.0.90",
            "injectJs": "true"
          },
          {
            "url": "apptkweb.latikait.com.ar",
            "injectJs": "true"
          }
        ],
    "cleartext": true,
  
    "androidScheme": "http"
  }
}
